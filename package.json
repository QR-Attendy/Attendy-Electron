{
  "name": "qr-attend-x",
  "productName": "QRttendX",
  "version": "0.1.1-a3",
  "description": "QRttendX made by ÆCE acebld",
  "license": "AGPL-3.0-only",
  "author": "QRttendX by ÆCE acebld",
  "type": "module",
  "main": "source/snapdragon.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.QRttendX.desk",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": "x64"
        }
      ],
      "icon": "main/media/icons/LOGO-ico.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": true,
      "installerIcon": "main/media/icons/LOGO-ico.ico",
      "uninstallerIcon": "main/media/icons/LOGO-ico.ico",
      "uninstallDisplayName": "QRttendX on Desktop",
      "license": "LICENSE"
    },
    "extraResources": [
      {
        "from": "main/pyAttendy/dist/attendy_engine.exe",
        "to": "atom/attendy_engine.exe"
      }
    ]
  },
  "dependencies": {
    "better-sqlite3": "^12.6.2",
    "electron-store": "^11.0.2"
  },
  "devDependencies": {
    "chokidar": "^5.0.0",
    "cross-env": "^10.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0"
  }
}
